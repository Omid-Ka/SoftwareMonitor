@using System.Web
<!-- Jquery Core Js -->
<script src="~/Admin/plugins/jquery/jquery.js"></script>

<script type="text/javascript">
    $(document).ready(function() {
        var success = '@Html.Raw(HttpUtility.JavaScriptStringEncode((string)TempData["Success"]))';    //'@TempData["Success"]';
        var warning = '@Html.Raw(HttpUtility.JavaScriptStringEncode((string)TempData["Warning"]))';    //'@TempData["Warning"]';
        var info = '@Html.Raw(HttpUtility.JavaScriptStringEncode((string)TempData["Info"]))';          //'@TempData["Info"]';
        var error = '@Html.Raw(HttpUtility.JavaScriptStringEncode((string)TempData["Error"]))';        //'@TempData["Error"]';

        if (success.length > 0) {
            toastr["success"](success);
        } else if (warning.length > 0) {
            toastr["warning"](warning);
        } else if (info.length > 0) {
            toastr["info"](info);
        } else if (error.length > 0) {
            toastr["error"](error);
        }
    });
</script>

