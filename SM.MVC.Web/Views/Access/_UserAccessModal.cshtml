@model List<Core.ViewModels.UserAccessVN>


<form id="UserAccessFrom">
    <div class="row clearfix">
        <div class="col-sm-3">
            @for (int i = 0; i < Model.Count; i++)
            {
                <div onclick="ClickOnGroup(@Model[i].AccessGroup.Id)">
                    <span>@Model[i].AccessGroup.Name</span>
                </div>
            }

        </div>
        <div class="col-sm-6">
            
            <div id="GroupDetail">

            </div>

        </div>
    </div>
</form>
