services:
  SqlDocker:
    container_name: DockerDB
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Omid3883
    image: mcr.microsoft.com/mssql/server:2019-CU15-ubuntu-20.04
    ports:
    - published: 14331
      target: 1433
  sm.mvc.web:
    build:
      context: D:\Omid\Project\SoftwareMonitor\SoftwareMonitor
      dockerfile: SM.MVC.Web/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: SM.MVC.Web
      target: base
    container_name: monitoring_api
    depends_on:
      SqlDocker:
        condition: service_started
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: monitoring/api:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/netcoreapp3.1/SM.MVC.Web.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 15422
      target: 80
    - target: 80
    - target: 443
    tty: true
    volumes:
    - D:\Omid\Project\SoftwareMonitor\SoftwareMonitor\SM.MVC.Web:/app:rw
    - C:\Users\internet\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\internet\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\internet\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\internet\.nuget\packages:/root/.nuget/packages:ro
    - D:\Omid\Project\SoftwareMonitor\SoftwareMonitor:/src:rw
version: '3.4'