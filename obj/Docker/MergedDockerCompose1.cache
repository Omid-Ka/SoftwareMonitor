services:
  sm.mvc.web:
    build:
      context: D:\Omid\Project\SoftwareMonitor\SoftwareMonitor
      dockerfile: SM.MVC.Web/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: smmvcweb
    ports:
